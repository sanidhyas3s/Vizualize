{
  "name": "vizualize",
  "displayName": "Vizualize",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vizualize.animate",
        "title": "Vizualize"
      }
