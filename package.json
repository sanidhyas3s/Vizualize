    "configuration": {
      "title": "Vizualize",
      "properties": {
        "vizualize.speed": {
          "title": "Animation Speed",
          "type": "number",
          "default": 750,
          "description": "The duration of each frame of animation in milliseconds."
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "child_process": "^1.0.2"
  }
}